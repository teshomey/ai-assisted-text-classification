   "source": [
    "from dataclasses import dataclass, field\n",
    "from typing import List, Optional\n",
    "\n",
    "@dataclass\n",
    "class PublicationRecord:\n",
    "\n",
    "    # WoS metadata\n",
    "    ut_id_wos: str\n",
    "    author_wos: str\n",
    "    title_wos: str\n",
    "    journal_wos: str\n",
    "    pub_year_wos: int\n",
    "    abstract_wos: Optional[str] = None\n",
    "    keywords_wos: Optional[str] = None\n",
    "    doi_wos: Optional[str] = None\n",
    "    \n",
    "    # OpenAlex metadata - for data enrichment\n",
    "    id_openalex: Optional[str] = None\n",
    "    title_openalex: Optional[str] = None\n",
    "    abstract_openalex: Optional[str] = None\n",
    "    concepts_openalex: List[str] = field(default_factory=list)\n",
    "    cited_by_count_openalex: Optional[int] = None\n",
    "    affiliation_details_openalex: List[dict] = field(default_factory=list)\n",
    "    #oa_status_openalex: str = 'Unlinked'\n",
    "    oa_match_method: Optional[str] = None  # 'doi', 'title_author', or 'none'\n",
    "    \n",
    "    # Classification outputs\n",
    "    classification_source_openalex: Optional[str] = None\n",
    "    classification_result: Optional[str] = None   "
   ]
