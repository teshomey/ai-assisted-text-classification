   "outputs": [],
   "source": [
    "# Clean and handle missing for 'keywords_wos'\n",
    "if 'keywords_wos' in df.columns:\n",
    "    # 1. Fill missing values (NaN) with an empty string ('')\n",
    "    # 2. Ensure the column is treated as a string type\n",
    "    # 3. Strip any leading/trailing whitespace\n",
    "    df['keywords_wos'] = df['keywords_wos'].fillna('').astype(str).str.lower().str.strip()\n",
    "    \n",
    "    # 4. Replace the now-cleaned empty strings with the Python object None\n",
    "    #    This signals that the value is genuinely missing, which is cleaner for the dataclass.\n",
    "    df['keywords_wos'] = df['keywords_wos'].replace('', None)"
   ]
