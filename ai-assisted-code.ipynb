    "openalex_works_url = 'https://api.openalex.org/works' # OpenAlex works API endpoint"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "20cd2281-61dd-480d-88b6-0a2b38b61dbc",
   "metadata": {},
   "source": [
    "### 2.2 Helper functions for OpenAlex."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 37,
   "id": "27695ad9-e639-410c-ab78-defec1404069",
   "metadata": {},
   "outputs": [],
   "source": [
    "# (a) Search by doi\n",
    "def search_openalex_by_doi(doi: str) -> Optional[dict]:\n",
    "    '''\n",
    "    Searches OpenAlex using the DOI identifier.\n",
    "    Returns: JSON response data for the work if found, else None.\n",
    "    '''\n",
    "    # Format: https://api.openalex.org/works/doi:doi_wos\n",
    "    url = f'{openalex_works_url}/doi:{doi}'\n",
    "    try:\n",
    "        response = requests.get(url)\n",
    "        if response.status_code == 200:\n",
    "            return response.json()\n",
    "    except requests.exceptions.RequestException as e:\n",
    "        print(f'DOI API Request Error for {doi}: {e}')\n",
    "    return None\n",
    "\n",
    "# (b) Search by title\n",
    "def search_openalex_by_title_author(title: str, authors: Optional[str]) -> Optional[Dict]:\n",
    "    '''\n",
    "    Search OpenAlex for a work using title and (optionally) the first author's last name.\n",
    "    \n",
    "    Args:\n",
    "        title (str): Title of the work to search.\n",
    "        authors (Optional[str]): Author list, e.g. 'Smith, John; Doe, Jane'.\n",
    "        \n",
    "    Returns:\n",
    "        Optional[dict]: First matching OpenAlex work record, or None.\n",
    "    '''\n",
    "    \n",
    "    # Base title search\n",
    "    filters = [f'title.search:{title}']\n",
    "    \n",
    "    # Try to extract first author's last name\n",
    "    if authors:\n",
    "        try:\n",
    "            first_author = authors.split(';')[0].strip()\n",
    "            last_name = first_author.split(',')[0].strip()\n",
    "            \n",
    "            if last_name:\n",
    "                filters.append(f'author.display_name:{last_name}')\n",
    "        except Exception:\n",
    "            pass  # Ignore parsing errors\n",
    "\n",
    "    # Prepare filter string\n",
    "    filter_str = ','.join(filters)\n",
    "\n",
    "    # Build the request URL\n",
    "    url = f'{openalex_works_url}?filter={filter_str}&per_page=1'\n",
    "\n",
    "    try:\n",
    "        response = requests.get(url, timeout=10)\n",
    "        response.raise_for_status()\n",
    "        \n",
    "        data = response.json()\n",
    "        \n",
    "        # Return first result if available\n",
    "        results = data.get('results', [])\n",
    "        return results[0] if results else None\n",
    "\n",
    "    except requests.RequestException as e:\n",
    "        print(f'[OpenAlex Error] {e}')\n",
    "        return None\n",
    "\n",
    "# (c) OpenAlex metadata extractor\n",
    "def process_openalex_record_data(oa_work_data: dict) -> dict:\n",
    "    '''\n",
    "    Extract and clean key metadata fields from an OpenAlex work record.\n",
    "    '''\n",
    "\n",
    "    # Concepts (simple list of concept display names)\n",
    "    concepts = [\n",
    "        concept.get('display_name')\n",
    "        for concept in oa_work_data.get('concepts', [])\n",
    "        if concept.get('display_name')\n",
    "    ]\n",
    "\n",
    "    # Affiliations (flatten all institutions across all authorships)\n",
    "    affiliations = [\n",
    "        {\n",
    "            'country_code': inst.get('country_code'),\n",
    "            'country': inst.get('country'),\n",
    "            'institution_name': inst.get('display_name')\n",
    "        }\n",
    "        for authorship in oa_work_data.get('authorships', [])\n",
    "        for inst in authorship.get('institutions', [])\n",
    "        if inst\n",
    "    ]\n",
    "\n",
    "    return {\n",
    "        'id_openalex': oa_work_data.get('id'),\n",
    "        'title_openalex': oa_work_data.get('title'),\n",
    "        'abstract_openalex': oa_work_data.get('abstract'), \n",
    "        'concepts_openalex': concepts,\n",
    "        'cited_by_count_openalex': oa_work_data.get('cited_by_count'),\n",
    "        'affiliation_details_openalex': affiliations\n",
    "    }\n",
    "\n",
    "# (d) OpenAlex record enricher\n",
    "def enrich_publication_data(record: PublicationRecord):\n",
    "    '''\n",
    "    Enrich a publication record using OpenAlex.\n",
    "    Priority search: DOI â†’ Title + Author.\n",
    "    Updates the record in place and returns it.\n",
    "    '''\n",
    "\n",
    "    # 1. Try doi match\n",
    "    oa_data = None\n",
    "    if record.doi_wos:\n",
    "        oa_data = search_openalex_by_doi(record.doi_wos)\n",
    "        if oa_data:\n",
    "            record. oa_match_method = 'doi'\n",
    "\n",
    "    # 2. Fallback: title + author\n",
    "    if oa_data is None:\n",
    "        oa_data = search_openalex_by_title_author(record.title_wos, record.author_wos)\n",
    "        if oa_data:\n",
    "            record.oa_match_method = 'title+author'\n",
    "        else:\n",
    "            record.oa_match_method = 'unlinked'\n",
    "            return record\n",
    "\n",
    "    # 3. Process OpenAlex data and update fields\n",
    "    processed = process_openalex_record_data(oa_data)\n",
    "    for key, value in processed.items():\n",
    "        setattr(record, key, value)\n",
    "\n",
    "    return record\n",
    "\n",
    "# (e) Prepare publication record from WoS dataframe for OpenAlex enrichment\n",
    "publication_list = [\n",
    "    PublicationRecord(\n",
    "        title_wos=row.get('title_wos', '').strip(),\n",
    "        journal_wos=row.get('journal_wos', '').strip(),\n",
    "        pub_year_wos=int(row['pub_year_wos']) if row.get('pub_year_wos') else None,\n",
    "        ut_id_wos=row.get('ut_id_wos', '').strip(),\n",
    "        author_wos=row.get('author_wos', None),\n",
    "        abstract_wos=row.get('abstract_wos', None),\n",
    "        keywords_wos=row.get('keywords_wos', None),\n",
    "        doi_wos=row.get('doi_wos', None)\n",
    "    )\n",
    "    for _, row in df.iterrows()\n",
    "]"
   ]
  },
